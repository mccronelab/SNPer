params {
    consensus_min_qual_score = 30
    consensus_threshold = 0.5
    consensus_min_depth = 10
}

profiles {
    dev {
        params{
            reference_fasta = './data/SNPer/data/sars_reference/MN908947.fasta'
            reference_gff = null
            primer_bed = './data/SNPer/data/primers/SARS-CoV-2_V4.1_primers_alt_overlap_sites.bed'
            primer_fasta = './data/SNPer/data/primers/SARS-CoV-2.primer.fa'
            primer_pairs = './data/SNPer/data/primers/SARS-CoV-2.primer_pairs.tsv'
            sample_sheet = './data/SNPer/testSampleSheet'
            output_dir = "./dev"

        }
        docker{
            enabled = false
        }
        process{
            executor='local'        
        }
        
    }
}